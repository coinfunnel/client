{
  "name": "coinfunnel",
  "productName": "CoinFunnel Client",
  "description": "Client for the CoinFunnel cryptocurrency funneling service",
  "version": "0.0.1",
  "private": true,
  "author": "CoinFunnel",
  "copyright": "Â© 2018, CoinFunnel",
  "homepage": "https://coinfunnel.org",
  "main": "app/background.js",
  "build": {
    "appId": "com.coinfunnel.client.v0.0.1",
    "productName": "CoinFunnel Client",
    "win": {
      "target": "nsis"
    },
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      "miner/config.json",
      "miner/start.cmd",
      "miner/xmrig.exe"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "node build/start.js",
    "pack": "webpack --config=build/webpack.app.config.js --env=development && electron-builder --win --x64 --dir",
    "release": "webpack --config=build/webpack.app.config.js --env=production && electron-builder --win --x64"
  },
  "dependencies": {
    "electron-store": "1.3.0",
    "fs-jetpack": "1.0.0",
    "glob": "7.1.2",
    "jquery": "3.3.1",
    "react": "16.3.1",
    "react-dom": "16.3.1",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-toggle-display": "2.2.0",
    "request": "2.85.0",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.5",
    "@babel/preset-env": "^7.0.0-beta.5",
    "@babel/preset-react": "^7.0.0-beta.44",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-object-rest-spread": "^7.0.0-beta.3",
    "css-loader": "^0.28.7",
    "electron": "1.8.2-beta5",
    "electron-builder": "^19.43.3",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "source-map-support": "^0.5.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  }
}
